{
  "name": "Retask",
  "version": "1.0",
  "description": "Email as Todo.",
  "content_scripts": [
    {
      "matches": ["https://mail.google.com/*"],
      "js": ["content.js"]
    }
  ],
  "web_accessible_resources": [
    "jquery-1.10.2.min.js",
    "gmail.js",
    "main.js"
  ],
  "manifest_version": 2,
  "icons": {
    "128": "icon128.png"
  }
}


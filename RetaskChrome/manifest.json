{
   "manifest_version": 2,
   "name": "Retask",
   "description": "Email as Todo.",
   "version": "1.0",
   "permissions": ["activeTab"],

   "homepage_url": "http://retask.io/",
   "content_scripts": [ {
      "js": ["gmail.js", "script.js"],
      "css": ["style.css"],
      "matches": ["*://mail.google.com/*"],
      "run_at": "document_end"
   }
   ],
  "icons": {
      "128": "icon128.png"
    }
}